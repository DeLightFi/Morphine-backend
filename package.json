{
  "name": "morphine-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "pre-commit": [
    "ts.check",
    "build",
    "copy-abi",
    "add-build"
  ],
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "build": "rimraf dist && tsc",
    "copy-abi": "cp -rv src/abi dist/abi/",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t morphine/mongodb .",
    "docker:start": "docker run --rm -d --name morphine-mongodb -v ./volumes/mongodb:/data/db -p 27017:27017 morphine/mongodb",
    "docker:stop": "docker stop morphine-mongodb"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apibara/protocol": "^0.4.3",
    "@apibara/starknet": "^0.2.1",
    "@types/cors": "^2.8.13",
    "@types/mongoose": "^5.11.97",
    "@types/node-schedule": "^2.1.0",
    "bn.js": "^5.2.1",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "mongoose": "^6.8.3",
    "node-schedule": "^2.1.0",
    "npm-cli": "^0.1.0",
    "starknet": "^4.17.1",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.1",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "pre-commit": "^1.2.2",
    "rimraf": "^4.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
